{
    "name": "@frontify/fondue",
    "version": "12.0.0-beta.21",
    "description": "Design system of Frontify",
    "engines": {
        "node": "~16"
    },
    "main": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "jsdelivr": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./tailwind": "./tailwind.config.js",
        "./style": "./dist/styles.css",
        "./extractedThirdPartyStyles": "./dist/extractedThirdPartyStyles.css",
        "./dist/*": "./dist/*"
    },
    "scripts": {
        "build": "npm run clean && npm run generate:icons && npm run build:library && npm run build:extractedThirdPartyStyles",
        "build:extractedThirdPartyStyles": "concat src/components/**/*.css | postcss > dist/extractedThirdPartyStyles.css",
        "build:library": "vite build",
        "build:storybook": "NODE_OPTIONS=--max_old_space_size=6144 build-storybook",
        "dev": "cross-env NODE_ENV=development npm run dev:library",
        "dev:library": "vite build --emptyOutDir false --watch",
        "clean": "rimraf dist",
        "component:create": "esno scripts/createNewComponent.ts",
        "generate:icons": "npm run generate:reactIcons && npm run generate:iconsMap",
        "generate:iconsMap": "esno ./scripts/generateIconMap.ts && eslint --fix ./src/foundation/Icon/IconEnum.ts ./src/foundation/Icon/IconsMap.tsx ./src/foundation/Icon/Generated/index.ts",
        "generate:reactIcons": "esno ./scripts/generateReactIcons.ts",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint:prettier": "prettier --check .",
        "test": "cypress run --component",
        "test:browser": "cypress open --component",
        "test:file": "cypress run --component --spec",
        "typecheck": "tsc --noEmit",
        "storybook": "start-storybook -p 6006"
    },
    "keywords": [
        "frontify",
        "react",
        "components",
        "design system"
    ],
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "url": "https://github.com/frontify/fondue"
    },
    "bugs": {
        "url": "https://github.com/frontify/fondue/issues"
    },
    "devDependencies": {
        "@cypress/vite-dev-server": "^3.1.1",
        "@frontify/eslint-config-react": "^0.15.5",
        "@frontify/fondue-icons": "^0.1.17",
        "@storybook/addon-actions": "6.5.13",
        "@storybook/addon-essentials": "6.5.13",
        "@storybook/addon-links": "6.5.13",
        "@storybook/builder-vite": "0.2.5",
        "@storybook/react": "6.5.13",
        "@svgr/core": "6.2.1",
        "@svgr/plugin-jsx": "6.2.1",
        "@svgr/plugin-prettier": "6.1.2",
        "@svgr/plugin-svgo": "6.2.0",
        "@types/react": "17.0.45",
        "@types/react-datepicker": "4.3.4",
        "@types/react-dom": "17.0.16",
        "@types/tinycolor2": "1.4.3",
        "autoprefixer": "10.4.7",
        "chalk": "5.0.1",
        "concat": "1.0.3",
        "cross-env": "7.0.3",
        "cypress": "^10.11.0",
        "cypress-real-events": "^1.7.1",
        "eslint": "8.27.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-notice": "0.9.10",
        "eslint-plugin-storybook": "^0.6.7",
        "esno": "0.16.3",
        "fast-glob": "3.2.11",
        "postcss": "8.4.14",
        "postcss-cli": "9.1.0",
        "postcss-import": "14.1.0",
        "postcss-nested": "5.0.6",
        "prettier": "2.7.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "rimraf": "3.0.2",
        "storybook-dark-mode": "1.1.2",
        "tailwindcss": "3.1.6",
        "ts-node": "10.9.1",
        "typescript": "4.7.4",
        "vite": "^3.0.3",
        "vite-plugin-dts": "1.4.0",
        "vite-plugin-eslint": "^1.7.0"
    },
    "dependencies": {
        "@frontify/fondue-tokens": "^3.1.0",
        "@popperjs/core": "^2.11.5",
        "@react-aria/accordion": "3.0.0-alpha.9",
        "@react-aria/aria-modal-polyfill": "3.6.0",
        "@react-aria/breadcrumbs": "3.1.10",
        "@react-aria/button": "3.6.2",
        "@react-aria/checkbox": "3.6.0",
        "@react-aria/combobox": "3.4.2",
        "@react-aria/dialog": "3.4.0",
        "@react-aria/focus": "3.9.0",
        "@react-aria/interactions": "3.12.0",
        "@react-aria/link": "3.3.4",
        "@react-aria/listbox": "3.7.0",
        "@react-aria/menu": "3.6.2",
        "@react-aria/overlays": "3.8.1",
        "@react-aria/radio": "3.4.0",
        "@react-aria/select": "3.8.2",
        "@react-aria/selection": "3.11.0",
        "@react-aria/table": "3.5.0",
        "@react-aria/tooltip": "3.3.2",
        "@react-aria/utils": "3.14.0",
        "@react-aria/visually-hidden": "3.5.0",
        "@react-stately/checkbox": "3.3.0",
        "@react-stately/collections": "3.4.4",
        "@react-stately/combobox": "3.2.2",
        "@react-stately/list": "3.5.4",
        "@react-stately/menu": "3.4.2",
        "@react-stately/overlays": "3.4.2",
        "@react-stately/radio": "3.6.0",
        "@react-stately/select": "3.3.2",
        "@react-stately/table": "3.5.0",
        "@react-stately/toggle": "3.4.2",
        "@react-stately/tooltip": "3.2.2",
        "@react-stately/tree": "3.3.4",
        "@storybook/addon-a11y": "^6.5.13",
        "@udecode/plate": "^18.9.0",
        "@xstate/immer": "^0.3.1",
        "@xstate/react": "^1.6.3",
        "date-fns": "^2.28.0",
        "escape-html": "^1.0.3",
        "framer-motion": "^6.4.3",
        "immer": "^9.0.12",
        "react-colorful": "^5.5.1",
        "react-datepicker": "^4.7.0",
        "react-dnd": "^15.1.1",
        "react-dnd-html5-backend": "^15.1.2",
        "react-is": "^18.1.0",
        "react-popper": "^2.3.0",
        "react-textarea-autosize": "^8.3.4",
        "remark-gfm": "^3.0.1",
        "remark-parse": "^10.0.1",
        "slate": "^0.78.0",
        "slate-history": "^0.66.0",
        "slate-hyperscript": "^0.77.0",
        "slate-react": "^0.79.0",
        "styled-components": "^5.3.5",
        "tinycolor2": "^1.4.2",
        "unified": "^10.1.2",
        "xstate": "^4.28.1"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
