{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "cypress": false,
    "entry": ["src/index.ts", "src/subpackages/*.ts"],
    "ignoreDependencies": [
        "@frontify/fondue-charts",
        "@frontify/fondue-components",
        "@frontify/fondue-icons",
        "@frontify/fondue-rte",
        "@frontify/fondue-tokens",
        "@svgr/plugin-jsx",
        "@svgr/plugin-svgo",
        "@svgr/plugin-prettier",
        "postcss-import",
        "ts-node",
        "@tailwindcss/forms",
        "commander",
        "execa",
        "eslint-plugin-tailwindcss"
    ],
    "ignoreExportsUsedInFile": {
        "interface": true,
        "type": true
    },
    "project": ["src/**/*.{ts,tsx,js,jsx}", "!src/**/*.cy.tsx", "!src/**/*.spec.{ts,tsx}"],
    "tags": ["-lintignore"]
}
