{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "target": "ES2022",
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "outDir": "dist/tools"
    },
    "include": ["tools/**/*"],
    "exclude": ["node_modules", "tools/transforms"]
}
