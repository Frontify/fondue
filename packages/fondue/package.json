{
    "name": "@frontify/fondue",
    "author": "Frontify Developers <developers@frontify.com>",
    "version": "12.0.0-beta.400",
    "description": "Design system of Frontify",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/Frontify/fondue",
        "directory": "packages/fondue"
    },
    "bugs": {
        "url": "https://github.com/frontify/fondue/issues"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "pnpm@8.6.0",
    "main": "dist/index.umd.cjs",
    "unpkg": "dist/index.umd.cjs",
    "jsdelivr": "dist/index.umd.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist",
        "tailwind.config.ts"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs"
        },
        "./tailwind": "./tailwind.config.ts",
        "./style": "./dist/style.css",
        "./extractedThirdPartyStyles": "./dist/extractedThirdPartyStyles.css",
        "./dist/*": "./dist/*"
    },
    "scripts": {
        "build": "pnpm clean && pnpm generate:icons && pnpm generate:componentsEnum && pnpm build:library && pnpm build:extractedThirdPartyStyles",
        "build:extractedThirdPartyStyles": "concat src/components/**/*.css | postcss > dist/extractedThirdPartyStyles.css",
        "build:library": "vite build",
        "build:storybook": "pnpm generate:icons && pnpm generate:componentsEnum && storybook build",
        "dev": "cross-env NODE_ENV=development pnpm dev:library",
        "dev:library": "vite build --emptyOutDir false --watch",
        "clean": "rimraf dist",
        "component:create": "tsx scripts/createNewComponent.ts",
        "generate:icons": "pnpm generate:reactIcons && pnpm generate:iconsEnum",
        "generate:iconsEnum": "tsx ./scripts/generateIconEnum.ts",
        "generate:componentsEnum": "tsx ./scripts/generateComponentEnum.ts",
        "generate:reactIcons": "tsx ./scripts/generateReactIcons.ts",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "lint:prettier": "prettier --check .",
        "test": "cypress run --component",
        "test:browser": "cypress open --component",
        "test:file": "cypress run --component --spec",
        "typecheck": "tsc --noEmit",
        "storybook": "storybook dev -p 6006",
        "ci": "pnpm install --frozen-lockfile"
    },
    "keywords": [
        "frontify",
        "react",
        "components",
        "design system"
    ],
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@cypress/vite-dev-server": "^5.0.7",
        "@frontify/eslint-config-react": "0.16.1",
        "@frontify/fondue-icons": "^0.1.42",
        "@storybook/addon-a11y": "^7.6.12",
        "@storybook/addon-actions": "^7.6.12",
        "@storybook/addon-essentials": "^7.6.12",
        "@storybook/addon-links": "^7.6.12",
        "@storybook/react": "^7.6.12",
        "@storybook/react-vite": "^7.6.12",
        "@storybook/theming": "^7.6.12",
        "@svgr/babel-plugin-transform-svg-component": "^6.5.1",
        "@svgr/core": "^6.5.1",
        "@svgr/plugin-jsx": "^6.5.1",
        "@svgr/plugin-prettier": "^6.5.1",
        "@svgr/plugin-svgo": "^6.5.1",
        "@types/escape-html": "^1.0.4",
        "@types/is-hotkey": "^0.1.10",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^18.19.11",
        "@types/react": "^18.2.48",
        "@types/react-datepicker": "^4.19.5",
        "@types/react-dom": "^18.2.18",
        "@types/react-is": "^18.2.4",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "chalk": "^5.3.0",
        "concat": "^1.0.3",
        "cross-env": "^7.0.3",
        "cypress": "^13.6.4",
        "cypress-real-events": "^1.11.0",
        "esbuild": "^0.20.0",
        "eslint": "^8.56.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-notice": "^0.9.10",
        "eslint-plugin-storybook": "^0.6.15",
        "fast-glob": "^3.3.2",
        "glob": "^10.3.10",
        "lodash-es": "^4.17.21",
        "postcss": "^8.4.33",
        "postcss-cli": "^11.0.0",
        "postcss-import": "^16.0.0",
        "postcss-nested": "^6.0.1",
        "prettier": "^3.2.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.9.6",
        "storybook": "^7.6.12",
        "storybook-dark-mode": "^3.0.3",
