{
    "name": "components",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/component-library.cjs",
    "module": "./dist/component-library.js",
    "exports": {
        "./content": {
            "import": "./.contentlayer/generated/index.mjs",
            "types": "./.contentlayer/generated/index.d.ts"
        },
        ".": {
            "import": "./dist/lib/component-lib.js",
            "types": "./dist/lib/component-lib.d.ts"
        }
    },
    "scripts": {
        "dev": "tsc && concurrently 'vite build --watch' 'contentlayer dev'",
        "build": "tsc && pnpm build:lib && contentlayer build ",
        "build:lib": "vite build",
        "preview": "vite preview"
    },
    "sideEffects": [
        "**/*.css"
    ],
    "devDependencies": {
        "@types/react": "^18.2.51",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-lib-inject-css": "^1.3.0"
    },
    "dependencies": {
        "@frontify/fondue-tokens": "3.4.0",
        "concurrently": "^8.2.2",
        "contentlayer": "^0.3.4",
        "postcss": "^8.4.33",
        "postcss-import": "^15.1.0",
        "react": "^18.2.0",
        "rollup": "^3.29.0",
        "tailwindcss": "^3.4.1",
        "vite-plugin-dts": "^3.5.3",
        "vite-plugin-watch": "^0.2.0"
    }
}
