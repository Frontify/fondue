        "@vitest/coverage-v8": "^2.1.4",
        "@vitest/ui": "^2.1.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-plugin-notice": "^1.0.0",
        "eslint-plugin-storybook": "^0.10.1",
        "happy-dom": "^14.12.3",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "storybook": "^8.3.6",
        "storybook-dark-mode": "^4.0.2",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vite-plugin-dts": "^4.3.0",
