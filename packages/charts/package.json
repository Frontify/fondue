        "./dist/*": "./dist/*"
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "vite build",
        "build:watch": "vite build --watch",
        "build:storybook": "storybook build",
        "storybook": "storybook dev -p 6008",
