{
    "name": "@fondue/docs",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "concurrently 'pnpm dev:contentlayer' 'pnpm dev:docs'",
        "dev:contentlayer": "contentlayer dev",
        "dev:docs": "vite",
        "build": "pnpm build:contentlayer && pnpm build:docs",
        "build:contentlayer": "contentlayer build",
        "build:docs": "vite build",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@frontify/fondue": "workspace:^",
        "@frontify/fondue-ui": "workspace:*",
        "@frontify/fondue-icons": "workspace:^",
        "@frontify/fondue-tokens": "^3.4.0",
        "@tailwindcss/forms": "^0.5.7",
        "lodash": "^4.17.21",
        "mdx-bundler": "^10.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-live": "^4.1.6",
        "vike": "^0.4.161",
        "vike-react": "^0.4.6"
    },
    "devDependencies": {
        "@frontify/eslint-config-react": "0.17.0",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.11.19",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "concurrently": "^8.2.2",
        "contentlayer": "^0.3.4",
        "eslint": "^8.56.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-notice": "^0.9.10",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "remark-gfm": "^3.0.1",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.3"
    }
